{
    "workbench.startupEditor": "newUntitledFile",
    "editor.wordWrap": "on",
    "files.autoSave": "afterDelay",
    "editor.fontFamily": "'Fira Code'",
    "editor.fontLigatures": true,
    "editor.insertSpaces": false,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "boundary",
    "window.zoomLevel": 1,
    "cSpell.language": ",fr",
    "workbench.colorTheme": "Cobalt2",
    "window.menuBarVisibility": "hidden",
    "javascript.updateImportsOnFileMove.enabled": "never",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontFamily": "Mononoki Nerd Font Mono",
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": true,
    "workbench.colorCustomizations": {
        "terminal.ansiBlack": "#000000",
        "terminal.ansiBrightBlack": "#555555",
        "terminal.ansiRed": "#ff0000",
        "terminal.ansiBrightRed": "#ff638a",
        "terminal.ansiGreen": "#38de21",
        "terminal.ansiBrightGreen": "#41d720",
        "terminal.ansiYellow": "#ffe50a",
        "terminal.ansiBrightYellow": "#fec51e",
        "terminal.ansiBlue": "#1460d2",
        "terminal.ansiBrightBlue": "#0186fd",
        "terminal.ansiMagenta": "#ff005d",
        "terminal.ansiBrightMagenta": "#fa95ff",
        "terminal.ansiCyan": "#00bbbb",
        "terminal.ansiBrightCyan": "#7cfcfd",
        "terminal.ansiWhite": "#ffffff",
        "terminal.ansiBrightWhite": "#bbbbbb",
        "terminal.background": "#182b3a",
        "terminal.foreground": "#ffffff",
        "terminalCursor.foreground": "#f1c423"
    }
}