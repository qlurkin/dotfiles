set nocompatible

filetype on
filetype plugin on
filetype indent on

syntax on

set lazyredraw
set showmode
set backspace=indent,eol,start
set encoding=utf-8
set relativenumber
set number
set cul
set tabstop=4 softtabstop=4
set shiftwidth=4
set smartindent
set noswapfile
set nobackup
set nowritebackup
set path+=**
set wildmenu
set hidden
set noerrorbells visualbell t_vb=
set belloff=all
